{
  "name": "@auth/astro",
  "version": "0.1.0",
  "description": "Authentication for Astro.",
  "keywords": [
    "authentication",
    "authjs",
    "jwt",
    "oauth",
    "oidc",
    "passwordless",
    "astro",
    "astro-component"
  ],
  "repository": "https://github.com/nextauthjs/next-auth.git",
  "author": "Otterlord <otterlord.dev@gmail.com>",
  "scripts": {
    "build": "tsup --config ./tsup.config.js && node scripts/postbuild",
    "patch": "npm version patch --no-git-tag-version"
  },
  "devDependencies": {
    "@types/cookie": "0.5.1",
    "@types/set-cookie-parser": "^2.4.2",
    "astro": "^2.0.0-beta.2",
    "next-auth": "latest",
    "tsup": "^6.5.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@auth/core": "latest",
    "set-cookie-parser": "^2.5.1"
  },
  "peerDependencies": {
    "astro": "^2.0.0-beta.2"
  },
  "type": "module",
  "types": "./index.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    },
    "./client": {
      "types": "./client.t.ts",
      "import": "./client.js"
    },
    "./components": {
      "types": "./components.ts",
      "import": "./components.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "client.*",
    "index.*",
    "components.*",
    "src"
  ]
}
